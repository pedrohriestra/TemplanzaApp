<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TemplanzaApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TemplanzaApp"
    Title="TemplanzaApp">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:LoginPage}"
        Route="LoginPage" />
    <ShellContent
    Title="AboutDev"
    ContentTemplate="{DataTemplate local:AboutDevPage}"
    Route="AboutDevPage" />

    <ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local:HomePage}"
    Route="HomePage" />

    <ShellContent
    Title="Service"
    ContentTemplate="{DataTemplate local:ServicePage}"
    Route="ServicePage" />


</Shell>
